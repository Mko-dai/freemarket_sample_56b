!!!
%html
  %head
  %body
    %header.header
      .menu_bar
        .headup
          .mercarilogo
            %a
              =image_tag "https://web-jp-assets.mercdn.net/_next/static/images/logo-acdd90ac4f472d5a6f7a330d33ab1225.svg", class: "logo_img", alt: "Mercari"
          %form.form{action: ""}
            %input.search_form{name: "keyword", placeholder:  "何かお探しですか？", type: "text", value: ""}/
            %button.search_button
              %svg.button_a{"aria-hidden" =>"true", height: "16", viewbox: "0 0 16 16", width: "16"}
                %path{d: "M11.435 10.063h-.723l-.256-.247a5.92 5.92 0 0 0 1.437-3.87 5.946 5.946 0 1 0-5.947 5.947 5.92 5.92 0 0 0 3.87-1.437l.247.256v.723L14.637 16 16 14.637l-4.565-4.574zm-5.489 0A4.111 4.111 0 0 1 1.83 5.946 4.111 4.111 0 0 1 5.946 1.83a4.111 4.111 0 0 1 4.117 4.116 4.111 4.111 0 0 1-4.117 4.117z", fill:  "currentColor", "fill-rule"=>"nonzero"}
        .headdown
          .search
            .search_category
              %a.search_category_link{href: "/jp/category/"}
                %svg.category_icon{"aria-hidden" =>"true", height: "16", viewbox: "0 0 20 16", width: "20"}
                  %path{:d => "M18.799 15.25h-11.5a1.25 1.25 0 1 1 0-2.5h11.5a1.25 1.25 0 0 1 0 2.5zm0-6h-11.5a1.25 1.25 0 1 1 0-2.5h11.5a1.25 1.25 0 0 1 0 2.5zm0-6h-11.5a1.25 1.25 0 1 1 0-2.5h11.5a1.25 1.25 0 0 1 0 2.5zM2 16a2 2 0 1 0 0-4 2 2 0 0 0 0 4zm0-6a2 2 0 1 0 0-4 2 2 0 0 0 0 4zm0-6a2 2 0 1 0 0-4 2 2 0 0 0 0 4z", fill: "currentColor", "fill-rule"=>"nonzero"}
                カテゴリーから探す
            .search_brand
              %a.search_brand_link{href: "/jp/brand/"}
                %svg.brand_icon{"aria-hidden" => "true", height: "16", viewbox: "0 0 23 16", width: "23"}
                  %path{d: "M6.315 0L4.473 1.806 2.833.019.73 1.977l1.69 1.84L1.198 5.02c-1.594 1.594-1.594 4.188-.01 5.772L6.3 16h13.272a3.08 3.08 0 0 0 3.077-3.077V3.076A3.08 3.08 0 0 0 19.57 0H6.315zm13.46 12.925a.211.211 0 0 1-.205.205H7.504L3.227 8.771a1.226 1.226 0 0 1-.01-1.71L4.38 5.92l1.28 1.371a2.68 2.68 0 1 0 2.13-1.93L6.433 3.907 7.49 2.87h12.082a.211.211 0 0 1 .206.205v9.848h-.002z", fill: "currentColor", "fill-rule" => "nonzero"}
                ブランドから探す
          .user_page_links
            %ul.user_links
              %li.like
                %a.like{href: "/jp/like/"}
                  %span いいね！一覧
                  %svg.sc-hSdWYo.sc-hrWEMg.elIJWw{"aria-hidden" => "true", height: "15", viewbox: "0 0 16 15", width: "16"}
                    %path{d: "M8.08 12.711l-.08.082-.08-.082C4.112 9.188 1.6 6.858 1.6 4.496c0-1.635 1.2-2.861 2.8-2.861 1.232 0 2.432.81 2.856 1.929h1.496c.416-1.12 1.616-1.93 2.848-1.93 1.6 0 2.8 1.227 2.8 2.862 0 2.362-2.512 4.692-6.32 8.215M11.6 0C10.208 0 8.872.662 8 1.708 7.128.662 5.792 0 4.4 0 1.936 0 0 1.978 0 4.496c0 3.09 2.72 5.608 6.84 9.433L8 15l1.16-1.079C13.28 10.104 16 7.586 16 4.496 16 1.978 14.064 0 11.6 0", fill: "currentColor", "fill-rule" => "evenodd"}
              %li.notice
                %a.notification{href: "/jp/notification/"}
                  %span お知らせ
                  %svg.sc-kgoBCf.sc-iQKALj.kjjLAK{"aria-hidden" => "true", height: "16", viewbox: "0 0 14 16", width: "14"}
                    %path{d: "M6.592 16c.89 0 1.632-.734 1.632-1.632H4.928c0 .898.742 1.632 1.664 1.632zm4.896-4.928V6.976c-.008-2.521-1.345-4.63-3.712-5.184v-.576C7.79.55 7.241 0 6.56 0c-.68 0-1.23.55-1.216 1.216v.576c-2.36.554-3.704 2.654-3.712 5.184v4.096L0 12.704v.832h13.12v-.832l-1.632-1.632zm-1.632.832h-6.56V6.976c0-2.024 1.238-3.68 3.296-3.68 2.026 0 3.264 1.656 3.264 3.68v4.928z", fill: "currentColor", "fill-rule" => "nonzero"}
              %li.todo
                %a.todoing{href: "/jp/todo/"}
                  %span やることリスト
                  %svg.sc-dxgOiQ.sc-bwCtUz.hkkZYC{"aria-hidden" => "true", height: "16", viewbox: "0 0 21 16", width: "21"}
                    %path{d: "M6.682 12.657L1.67 7.642 0 9.313 6.682 16 21 1.672 19.33 0z", fill: "currentColor", "fill-rule" => "nonzero"}
              %li.mypage
                %a.mypage_font{href: "/jp/mypage/"}
                  %span マイページ
                  =image_tag "/assets/member_photo_noimage.png", class: "mypage_img"
    %main.main_contents
      .main
        .upper
          .mypage_name
            = image_tag "/assets/member_photo_noimage.png", class: "border_radius"
            = link_to "ユーザーネーム", "evaluation.html"
            %ul
              評価
              %span.math 0
            %ul
              出品数
              %span.math 0
          .mypage_tabs
            %input#notice{checked: "checked", name: "tab_item1", type: "radio"}/
            %label.tab_item1{for: "notice"} お知らせ
            %input#todo{name:  "tab_item1", type: "radio"}/
            %label.tab_item1{for: "todo"} やることリスト
            #notice_contents.tab_contents
              = image_tag "/assets/mercari_icon.png", class: "mercari_icon"
              %p 実際の通知
            #todo_contents.tab_contents
              = image_tag "/assets/mercari_icon.png", class: "mercari_icon"
              %p 現在、やることリストはありません
            .list
              %ul 一覧をみる
          .lower
            %ul 購入した商品
            .lower_tabs
              %input#transaction{checked: "checked", name: "tab_item2", type: "radio"}/
              %label.tab_item2{:for => "transaction"} 取引中
              %input#past_transaction{name: "tab_item2", type: "radio"}/
              %label.tab_item2{for: "past_transaction"} 過去の取引
              #transaction_contents.tab_contents
                = image_tag "/assets/mercari_icon.png", class: "mercari_icon"
                %p 取引中の商品がありません
              #past_transaction_contents.tab_contents
                = image_tag "/assets/mercari_icon.png", class: "mercari_icon"
                %p 過去に取引した商品がありません
      .side_bar
        .side_bar_name
          %ul.header_lists
            %li
              マイページ
              %i.fas.fa-chevron-right
            %li
              お知らせ
              %i.fas.fa-chevron-right
            %li
              やることリスト
              %i.fas.fa-chevron-right
            %li
              いいね！一覧
              %i.fas.fa-chevron-right
            %li
              出品する
              %i.fas.fa-chevron-right
            %li
              出品した商品-出品中
              %i.fas.fa-chevron-right
            %li
              出品した商品-取引中
              %i.fas.fa-chevron-right
            %li
              出品した商品-売却済み
              %i.fas.fa-chevron-right
            %li
              購入した商品-取引中
              %i.fas.fa-chevron-right
            %li
              購入した商品-過去の取引
              %i.fas.fa-chevron-right
            %li
              ニュース一覧
              %i.fas.fa-chevron-right
            %li
              評価一覧
              %i.fas.fa-chevron-right
            %li
              ガイド
              %i.fas.fa-chevron-right
            %li
              お問い合わせ
              %i.fas.fa-chevron-right
          %h4
            メルペイ
          %ul.header_merpay
            %li
              売上・申込申請
              %i.fas.fa-chevron-right
            %li
              ポイント
              %i.fas.fa-chevron-right
          %h4
            設定
          %ul.header_set
            %li
              プロフィール
              %i.fas.fa-chevron-right
            %li
              発送元・お届け先住所変更
              %i.fas.fa-chevron-right
            %li
              支払い方法
              %i.fas.fa-chevron-right
            %li
              メール/パスワード
              %i.fas.fa-chevron-right
            %li
              本人情報
              %i.fas.fa-chevron-right
            %li
              電話番号の確認
              %i.fas.fa-chevron-right
            %li
              ログアウト
              %i.fas.fa-chevron-right
    %aside.bottom_banner
      .bottom_banner_pc
        .bottom_banner_pc_block
          %h2 スマホでかんたんフリマアプリ
          %p 今すぐ無料ダウンロード！
          .bottom_banner_pc_icon
            %figure
              = image_tag "/assets/mercari_icon.png", class: "marcari_icon_pc"
            %ul.app_icon
              %li
              = link_to image_tag("/assets/AppStore.png",  class: "AppStore"), "https://itunes.apple.com/jp/app/id667861049?l=ja&mt=8"
              %li
              = link_to image_tag("/assets/google-play-badge.png", class: "google-play-badge"), "https://play.google.com/store/apps/details?id=com.kouzoh.mercari&hl=ja"
        %figure
          = image_tag "/assets/download_content_pc.png"
    %footer
      .footer
        .top
          .first
            %h2.title メルカリについて
            %ul.content
              %li.companyProfile
                = link_to "会社概要（運営会社）", "https://about.mercari.com/", class: "mercari"
              %li.employmentInformation
                = link_to "採用情報", "/jobs/", class: "jobs"
              %li.pre-release
                = link_to "プレリリース", "https://about.mercari.com/press/news/", class: "press"
              %li.officialBlog
                = link_to "公式ブログ", "https://jp-news.mercari.com/", class: "blog"
              %li.logo-usage-guidelines
                = link_to "メルカリロゴ利用ガイドライン", "/logo-guidelines/", class: "guidelines"
              = link_to "https://twitter.com/mercari_jp", class: "twitter" do
                %i.fab.fa-twitter
              = link_to "https://www.facebook.com/mercarijp", class: "facebook" do
                %i.fab.fa-facebook-square
          .second
            %h2.titleH ヘルプ&amp;ガイド
            %ul.contentS
              %li.liS
                = link_to "メルカリガイド", "/help_center/", class: "mercari"
              %li.liS
                = link_to "らくらくメルカリ便", "rakuraku-mercari", class: "jobs"
              %li.liS
                = link_to "ゆうゆうメルカリ便", "/yuyu-mercari/", class: "press"
              %li.liS
                = link_to "大型メルカリ便", "/rakuraku-ogata/", class: "blogs"
              %li.liS
                = link_to "車体取引ガイド", "/guide/car/", class: "guidelines"
              %li.liS
                = link_to "メルカリあんしん・あんぜん宣言！", "/safe/description/", class: "press"
              %li.liS
                = link_to "偽ブランド品撲滅への取り組み", "/authenticity/", class: "blog"
              %li.liSmeri
                = link_to "メルカリボックス", "/box/", class: "guidelines"
          .third
            %h2.titleP プライバシーと利用規約
            .hidari
              %ul.contentT
                %li.liT
                  = link_to "プライバシーポリシー", "/privacy/", class: "mercari"
                %li.liT
                  = link_to "メルカリ利用規約", "/tos/", class: "jobs"
                %li.liT
                  = link_to "あんしんスマホサポート制度に関する利用規約", "/tos_smartphone_support/", class: "press"
                %li.liT
                  = link_to "コンプライアンスポリシー", "/compliance/", class: "blog"
            .migi
              %ul.contentT
                %li.liT
                  = link_to "個人データの安全管理に係る基本方針", "/data_security/", class: "mercari"
                %li.liT
                  = link_to "特定商取引に関する表記", "/tokutei/", class: "jobs"
                %li.liT
                  = link_to "資本決済法に基づく表示", "/shikin_kessai/", class: "press"
                %li.liT
                  = link_to "法令遵守と犯罪防止のために", "https://about.mercari.com/csr/safety/", class: "blog"
          .fourth
            %h2.country 国
            %ul.c
              %li.nippon
                = link_to "日本", "/", class: "n"
              %li.nip
                = link_to "United States", "/", class: "n"
        .bottom
          = link_to image_tag("https://web-jp-assets.mercdn.net/_next/static/images/logo-white-1a0696cf557ee1ed6f3c3444661d21ad.svg", class: "sc-kNBZmU.igKrOd", alt: "Mercari"), "/", class: "logo"
          %span.trademark © 2019 Mercari
    = link_to "/sell/", class: "sell_btn" do
      = "出品"
      %i.fas.fa-camera